{
  "name": "Học Tiếng Anh",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Ứng dụng học tiếng anh, ghi nhớ và nhắc từ vựng được triển bởi AlphaL",
  "permissions": [ "storage", "activeTab", "*://*/*" ],
  "browser_action": {
      "default_icon": "images/logo.png",
      "default_popup": "views/popup.html"
   },
   "content_scripts": [ {
      "css": [ "css/learnEglishDLMain.css" ],
      // "js": [ "js/jquery-3.3.1.min.js", "js/learnEglishDLMain.js"],
      "js": [ "js/learnEglishDLMain.js"],
      "matches": [ "*://*/*" ]
   } ]
}
